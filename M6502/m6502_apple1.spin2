                setbyte 0-0, #$20, #0
                add     _fifo_tail, #1
                incmod  _fifo_x, #40-1  wc
        if_nc   jmp     #.l2
                sub     _fifo_tail, #40

                add     _fifo_head, #40
                cmpsub  _fifo_head, ##24 * 40

