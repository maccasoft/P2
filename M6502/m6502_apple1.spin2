' I/O Routines

_read
                cmp     t2, ##$D010     wz
        if_z    jmp     #.kbd
                cmp     t2, ##$D011     wz
        if_z    jmp     #.kbdcr
                cmp     t2, ##$D012     wz
        if_nz   cmp     t2, ##$D013     wz
        if_z    jmp     #.pia

                getword t5, t2, #0
                add     t5, ram_addr
        _ret_   rdbyte  t1, t5

.kbd            rdpin   t1, #UART_RX
                shr     t1, #24
                cmp     t1, #"a"        wc
        if_nc   cmpr    t1, #"z"        wc
        if_nc   xor     t1, #%00100000
        _ret_   or      t1, #%10000000

.kbdcr          mov     t1, #0
                testp   #UART_RX        wz
        _ret_   bitz    t1, #7

.pia            mov     t5, t2
                sub     t5, ##$CE14
        _ret_   rdlut   t1, t5

_write
                cmp     t2, ##$D010     wz
        if_nz   cmp     t2, ##$D011     wz
        if_nz   cmp     t2, ##$D012     wz
        if_nz   cmp     t2, ##$D013     wz
        if_z    jmp     #.pia

                cmp     t2, ##ram_top_6502   wc
        if_nc   ret

                getword t5, t2, #0
                add     t5, ram_addr
        _ret_   wrbyte  t1, t5

.pia            mov     t5, t2
                sub     t5, ##$CE14
        _ret_   wrlut   t1, t5

i_halt          ret


'
'
' VGA Driver
'
DAT             org     $000

vga_driver
                add     ptrb, ##@vga_lut - @vga_driver
                setq2   #(@vga_lut_end - @vga_lut) / 4 -1   ' load chr rom and palette into lut
                rdlong  0, ptrb

                setluts #1                  ' enable lut sharing with cog #0
                cogatn  #%0000000_1         ' signal we are ready

                mov     pa, ##_CLKFREQ      ' calculate streamer frequency
                qfrac   ##VGA_FPIX, pa
                getqx   pa
                shr     pa, #1
                setxfrq pa

                setcy   ##VGA_INTENSITY << 24   ' r set colorspace for rgb
                setci   ##VGA_INTENSITY << 16   ' g
                setcq   ##VGA_INTENSITY << 08   ' b

                setcmod #%01_0_000_0        ' enable colorspace conversion

                cogid   _vx                 ' insert cogid into dac modes
                setnib  dacmode_s, _vx, #2
                setnib  dacmode_c, _vx, #2

                wrpin   dacmode_s, _pn      ' enable 123-ohm 3.3V dac mode in pin +0

                xor     _pn, #2 << 6 | 1    ' enable 75-ohm 2.0V dac mode in pins +1..3
                wrpin   dacmode_c,_pn

                xor     _pn,#3<<6 ^(2<<6|1) ' make pins +0..3 outputs
                drvl    _pn

                xor     _pn,#4|(3<<6)       ' leave av_base pointing to pin +4 (vertical sync)

                mov     ijmp1, #field       ' set up streamer-empty interrupt
                setint1 #10
                xcont   #10, #0             ' do streamer instruction to start interrupt sequence

'
'
' Field loop
'

field           callpa  #24, #border

                mov     _sl, #0
                mov     _vy, #192           ' visible lines
                mov     _lb, _fifo_head

.line           call    #line
                call    #line
                incmod  _sl, #7         wc
        if_c    add     _lb, #40
                cmpsub  _lb, ##24 * 40

                djnz    _vy, #.line

                callpa  #24, #border

                ' first blank line done here to use visible
                ' pixels time to update the fifo

                xcont   _as, #0             ' do before-visible part of scan line
                xcont   _vi, #0             ' do visible part of scan line (blank)

                incmod  _fc, #60-1          ' cursor blink timer
                cmp     _fc, #40        wc  ' |
        if_c    mov     _ch, #$00           ' |
        if_nc   mov     _ch, #$20           ' |
                altsb   _fifo_tail, #_fifo  ' |
                setbyte 0-0, _ch, #0        ' |

