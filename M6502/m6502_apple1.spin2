                xcont   _vi, #0             ' do visible part of scan line (blank)
                xcont   _bs, #0             ' do before-sync part of scan line
                xzero   _ds, #1             ' do sync part of scan line

                xcont   _as, #0             ' do before-visible part of scan line
                xcont   _vi, #0             ' do visible part of scan line (blank)
                xcont   _bs, #0             ' do before-sync part of scan line
                xzero   _ds, #1             ' do sync part of scan line
        _ret_   djnz    pa, #border

